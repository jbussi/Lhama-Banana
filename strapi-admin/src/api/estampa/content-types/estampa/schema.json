{
  "kind": "collectionType",
  "collectionName": "estampa",
  "info": {
    "singularName": "estampa",
    "pluralName": "estampas",
    "displayName": "Estampa",
    "description": "Estampas dispon√≠veis para produtos"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "nome": {
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 100
    },
    "descricao": {
      "type": "text"
    },
    "imagem_url": {
      "type": "string",
      "required": true,
      "maxLength": 2000
    },
    "categoria": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::categoria.categoria",
      "required": true
    },
    "tecido": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tecido.tecido"
    },
    "sexo": {
      "type": "enumeration",
      "enum": ["m", "f", "u"],
      "default": "u"
    },
    "ativo": {
      "type": "boolean",
      "default": true
    },
    "ordem_exibicao": {
      "type": "integer",
      "default": 0
    }
  }
}


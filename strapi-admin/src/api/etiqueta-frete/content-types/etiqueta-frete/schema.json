{
  "kind": "collectionType",
  "collectionName": "etiquetas_frete",
  "info": {
    "singularName": "etiqueta-frete",
    "pluralName": "etiqueta-fretes",
    "displayName": "Etiqueta de Frete",
    "description": "Etiquetas de frete para envio de pedidos - Gestão completa de embalagem e impressão"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "venda": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::venda.venda",
      "inversedBy": "etiquetas_frete"
    },
    "codigo_pedido": {
      "type": "string",
      "maxLength": 50
    },
    "melhor_envio_shipment_id": {
      "type": "integer"
    },
    "melhor_envio_protocol": {
      "type": "string",
      "maxLength": 100
    },
    "melhor_envio_service_id": {
      "type": "integer"
    },
    "melhor_envio_service_name": {
      "type": "string",
      "maxLength": 100
    },
    "status_etiqueta": {
      "type": "enumeration",
      "enum": [
        "pendente",
        "criada",
        "paga",
        "impressa",
        "cancelada",
        "erro",
        "em_transito",
        "entregue"
      ],
      "default": "pendente",
      "required": true
    },
    "transportadora_nome": {
      "type": "string",
      "maxLength": 100
    },
    "transportadora_codigo": {
      "type": "string",
      "maxLength": 50
    },
    "cep_origem": {
      "type": "string",
      "required": true,
      "maxLength": 10
    },
    "cep_destino": {
      "type": "string",
      "required": true,
      "maxLength": 10
    },
    "peso_total": {
      "type": "decimal",
      "min": 0
    },
    "valor_frete": {
      "type": "decimal",
      "min": 0
    },
    "dimensoes": {
      "type": "json"
    },
    "url_etiqueta": {
      "type": "text"
    },
    "url_rastreamento": {
      "type": "text"
    },
    "codigo_rastreamento": {
      "type": "string",
      "maxLength": 100
    },
    "dados_etiqueta_json": {
      "type": "json"
    },
    "erro_mensagem": {
      "type": "text"
    },
    "erro_detalhes": {
      "type": "json"
    },
    "paga_em": {
      "type": "datetime"
    },
    "impressa_em": {
      "type": "datetime"
    },
    "enviada_em": {
      "type": "datetime"
    },
    "entregue_em": {
      "type": "datetime"
    }
  }
}




{
  "kind": "collectionType",
  "collectionName": "categorias",
  "info": {
    "singularName": "categoria",
    "pluralName": "categorias",
    "displayName": "Categoria",
    "description": "Categorias de produtos e estampas"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "nome": {
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 50
    },
    "descricao": {
      "type": "text"
    },
    "ordem_exibicao": {
      "type": "integer",
      "default": 0
    },
    "ativo": {
      "type": "boolean",
      "default": true
    },
    "imagem_url": {
      "type": "string",
      "maxLength": 2000
    },
    "criado_por": {
      "type": "integer"
    },
    "atualizado_por": {
      "type": "integer"
    },
    "nome_produtos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::nome-produto.nome-produto",
      "mappedBy": "categoria"
    },
    "estampas": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::estampa.estampa",
      "mappedBy": "categoria"
    }
  }
}

